                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            
            <xs:element name="getgenreMoviesListResponse" type="tns:genreMoviesListType"/>
            
            <xs:element name="addgenreMoviesRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="token" type="xs:string"/>
                        <xs:element name="movieId" type="xs:integer"/>
