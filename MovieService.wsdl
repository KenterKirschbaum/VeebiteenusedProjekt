				</xs:complexType>
			</xs:element>
            
			<xs:element name="getMovieListResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" maxOccurs="unbounded" name="movie" type="tns:movieType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
            
			<xs:element name="getGenreRequest">
				<xs:complexType>
					<xs:sequence>
						<xs:element name="token" type="xs:string"/>
