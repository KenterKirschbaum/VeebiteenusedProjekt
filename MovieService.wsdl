				    <xs:element name="genreName" minOccurs="1">
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:pattern value="Action|Horror|Comedy"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
                    <xs:element name="genreMoviesList" type="tns:genreMoviesListType"/>
				</xs:sequence>
			</xs:complexType>
            
			<xs:element name="getMovieRequest">
