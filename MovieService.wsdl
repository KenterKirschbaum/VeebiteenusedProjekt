				</xs:complexType>
			</xs:element>
            
			<xs:element name="getGenreListResponse">
				<xs:complexType>
					<xs:sequence>
						<xs:element minOccurs="0" maxOccurs="unbounded" name="genre" type="tns:genreType"/>
					</xs:sequence>
				</xs:complexType>
			</xs:element>
            
			<xs:element name="getgenreMoviesListRequest">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="token" type="xs:string"/>
